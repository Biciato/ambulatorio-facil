<div id="cadastroMenu" class="card card-cascade narrower mt-5">
    <div class="view view-cascade gradient-card-header blue-gradient">
        <h2 class="card-header-title">Cadastro</h2>
    </div> 
    <div class="card-body">  
        <form id="cadastroForm">
            <h4 class="formTitle">Dados Cadastrais da Empresa</h4>
            <div class="form-row">
                <div class="col-3">
                    <div class="md-form">
                        <input type="text" id="cnpj" class="form-control">
                        <label for="cnpj" >CNPJ</label>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col">
                    <div class="md-form">
                        <input type="text" id="razaoSocial" class="form-control">
                        <label for="razaoSocial" >Razão Social</label>
                    </div>
                </div>
                <div class="col">
                    <div class="md-form">
                        <input type="text" id="nomeFantasia" class="form-control">
                        <label for="nomeFantasia" >Nome Fantasia</label>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col">
                    <div class="md-form">
                        <input type="text" id="ie" class="form-control">
                        <label for="ie" >Inscrição Estadual</label>
                    </div>
                </div>
                <div class="col">
                    <div class="md-form">
                        <input type="text" id="cnae" class="form-control">
                        <label for="cnae" >CNAE Principal</label>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col">
                    <div class="md-form">
                        <input type="text" id="form1" class="form-control">
                        <label for="form1" >E-mail</label>
                    </div>
                </div>
                <div class="col">
                    <div class="md-form">
                        <input type="text" id="form1" class="form-control">
                        <label for="form1" >E-mail financeiro</label>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-3">
                    <div class="md-form">
                        <input type="text" id="tel" class="form-control">
                        <label for="tel" >Telefone</label>
                    </div>
                </div>
            </div>
            <hr class="w-100">
            <h4 class="formTitle mt-5">Endereço</h4>
            <div class="form-row">
                <div class="col-3">
                    <div class="md-form">
                        <input type="text" id="cep" class="form-control">
                        <label for="cep" >CEP</label>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-7">
                    <div class="md-form">
                        <input type="text" id="logradouro" class="form-control">
                        <label for="logradouro" >Logradouro</label>
                    </div>
                </div>
                <div class="col-1">
                    <div class="md-form">
                        <input type="text" id="num" class="form-control">
                        <label for="num" >Número</label>
                    </div>
                </div>
                <div class="col-4">
                    <div class="md-form">
                        <input type="text" id="compl" class="form-control">
                        <label for="compl" >Complemento</label>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-5">
                    <div class="md-form">
                        <input type="text" id="bairro" class="form-control">
                        <label for="bairro" >Bairro</label>
                    </div>
                </div>
                <div class="col-5">
                    <div class="md-form">
                        <input type="text" id="cid" class="form-control">
                        <label for="cid" >Cidade</label>
                    </div>
                </div>
                <div class="col-2">
                    <div class="md-form">
                        <input type="text" id="est" class="form-control">
                        <label for="est" >Estado</label>
                    </div>
                </div>
            </div>
            <button type="button" class="btn btn-rounded btn-primary float-right mb-3 mr-1"><i class="fa fa-floppy-o pr-2" aria-hidden="true"></i>Salvar</button>      
        </form>
    </div>
</div>